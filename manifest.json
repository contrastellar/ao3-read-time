{

    "description": "A Firefox extension that is centered around showing the time it'll take to read a fic, depending on the word count. ",
    "manifest_version": 3,
    "name": "ao3-read-time",
    "version": "0.1.0",
    "homepage_url": "https://github.com/contrastellar/ao3-read-time",
    "icons": {
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.archiveofourown.org/*"],
        "js": ["./src/ao3-read-time.js"]
      }
    ],

    "browser_specific_settings": {
        "gecko": {
            "id": "gabriella@contrastellar.com"
        }
    }
  
}